{
  "repository": "https://github.com/singulart",
  "license": "MIT",
  "scripts": {
    "generate": "graphql-codegen && graphql-codegen --config codegen-atlas.yml",
    "start": "npm run generate && nest start",
    "prebuild": "npm run generate && rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discord-nestjs/common": "^3.3.4",
    "@discord-nestjs/core": "^3.3.4",
    "@golevelup/nestjs-graphql-request": "^0.1.11",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-graphql-request": "^4.4.10",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@joystream/types": "0.19.3",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.0.1",
    "axios": "^0.27.2",
    "cache-manager": "^4.1.0",
    "crypto": "^1.0.1",
    "discord.js": "^13.8.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "install": "^0.13.0",
    "moment-duration-format": "^2.3.2",
    "nanoid": "^3.3.4",
    "npm": "^8.14.0",
    "pg": "^8.7.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "sequelize": "^6.21.0",
    "sequelize-typescript": "^2.1.3",
    "typescript-retry-decorator": "^2.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/cache-manager": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.3"
  }
}
